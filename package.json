    "react-dom": "^19"
  },
  "devDependencies": {
    "@babylonjs/inspector": "^8.52.0",
    "@tsconfig/recommended": "^1.0.13",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bun-lightningcss": "^0.2.0",
    "web-ifc": "^0.0.75"
  }
